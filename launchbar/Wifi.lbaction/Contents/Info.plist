  You can find icons via http://osxdaily.com/2014/07/27/mac-os-x-system-icons-location/
-->
<plist version="1.0">
<dict>
  <key>CFBundleIdentifier</key>        <string>com.pea53.LaunchBar.action.Wifi</string>
  <key>CFBundleName</key>              <string>Wifi</string>
  <key>CFBundleVersion</key>           <string>1.0.0</string>
  <key>CFBundleIconFile</key>          <string>wifi</string>
  <key>LBDescription</key>
  <dict>
    <key>LBSummary</key>      <string>Turn the AirPort wifi on and off</string>
    <key>LBRequirements</key> <string>A computer with an AirPort wifi card is required to use this action.</string>
    <key>LBAuthor</key>       <string>Tim Pease</string>
    <key>LBEmail</key>        <string>tim.pease@gmail.com</string>
    <key>LBTwitter</key>      <string>@pea53</string>
    <key>LBWebsiteURL</key>   <string>http://pea53.com</string>
  </dict>
  <key>LBScripts</key>
  <dict>
    <key>LBDefaultScript</key>
    <dict>
      <key>LBScriptName</key>          <string>wifi.js</string>
      <key>LBRunInBackground</key>     <false/>
      <key>LBKeepWindowActive</key>    <false/>
      <key>LBLiveFeedbackEnabled</key> <false/>
      <key>LBRequiresArgument</key>    <false/>
      <key>LBReturnsResult</key>       <true/>
      <key>LBResultType</key>          <string></string>
    </dict>
  </dict>
  <key>LSMinimumSystemVersion</key>   <string>10.11</string>
  <key>NSHumanReadableCopyright</key> <string>Copyright ©️ 2016 Tim Pease</string>
</dict>
</plist>
