<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<!--
  See the LaunchBar developer documentation for descriptions of all these fields
  https://developer.obdev.at/launchbar-developer-documentation/#/action-info-plist
-->
<plist version="1.0">
<dict>
  <key>CFBundleIdentifier</key>        <string>com.pea53.LaunchBar.action.GitHub</string>
  <key>CFBundleName</key>              <string>GitHub</string>
  <key>CFBundleVersion</key>           <string>1.0.0</string>
  <key>LBAbbreviation</key>            <string>github</string>
  <key>CFBundleIconFile</key>          <string>font-awesome:fa-github</string>
  <key>LBAssociatedApplication</key>   <string></string>
  <key>LBRequiredApplication</key>     <string></string>
  <key>LBMinimumLaunchBarVersion</key> <integer>6132</integer>
  <key>LBDebugLogEnabled</key>         <false/>
  <key>LBDescription</key>
  <dict>
    <key>LBSummary</key>
    <string>Converts GitHub URLs to a markdown friendly format.</string>
    <key>LBRequirements</key> <string></string>
    <key>LBArgument</key>     <string>GitHub Issue/PR URL</string>
    <key>LBResult</key>       <string></string>
    <key>LBAuthor</key>       <string>Tim Pease</string>
    <key>LBEmail</key>        <string>tim.pease@gmail.com</string>
    <key>LBTwitter</key>      <string>@pea53</string>
    <key>LBWebsiteURL</key>   <string>http://pea53.com</string>
  </dict>
  <key>LBScripts</key>
  <dict>
    <key>LBDefaultScript</key>
    <dict>
      <key>LBAcceptedArgumentTypes</key> <array><string>string</string></array>
      <key>LBScriptName</key>            <string>github.js</string>
      <key>LBRunInBackground</key>       <false/>
      <key>LBKeepWindowActive</key>      <false/>
      <key>LBLiveFeedbackEnabled</key>   <false/>
      <key>LBRequiresArgument</key>      <false/>
      <key>LBReturnsResult</key>         <true/>
      <key>LBResultType</key>            <string></string>
      <key>LBAllowsInstantReuseOfLastTextInputString</key> <false/>
    </dict>
  </dict>
  <key>LBTextInputTitle</key>         <string>GitHub Issue/PR URL</string>
  <key>LSMinimumSystemVersion</key>   <string>10.11</string>
  <key>NSHumanReadableCopyright</key> <string>Copyright ©️ 2018 Tim Pease</string>
</dict>
</plist>
